//Intermediate Devices (Transcriptional Units)
Device Inverter = 
    ConstPr + RBS + Repressor + Terminator +
    RepPr + RBS + Reporter + Terminator;

Rule r(
    ON Inverter:
        Inverter[2].Represses EQUALS Inverter[4].Name
        AND
        B0030m WITH B0031m
        AND 
        B0030m BEFORE B0031m 
        AND
        Inverter[6].Name EQUALS "E1010m" 
);

Device[] lst = product(Inverter, strict);
save(lst);
